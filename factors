#!/usr/bin/python3

import sys
import time

def factorize(num):
    '''
    Takes a number as input and returns a tuple of two factors if the number is not a prime, or None if the number is prime.
    Args:
        num: input integer.
    Return:
        A tuple of two factors if the number is not a prime. None if the number is prime.
    '''
    for i in range(2, int(num ** 0.5) + 1):
        if num % i == 0:
            return (i, num // i)
    return None

if __name__ == "__main__":
    if len(sys.argv) != 2:
        print("Usage: factors <file>")
        sys.exit(1)

    input_file = sys.argv[1]

    try:
        with open(input_file, 'r') as f:
            lines = f.read().splitlines()
    except FileNotFoundError:
        print("File not found")
        sys.exit(1)

    start_time = time.time()

    for line in lines:
        num = int(line.strip())
        factors = factorize(num)
        if factors:
            print(f"{num} = {factors[0]} * {factors[1]}")

        # Check for time limit
        if time.time() - start_time > 5:
            print("Time limit exceeded")
            sys.exit(1)

